<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nicholas.wx.entity.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.juyou.wx.entity.dto.User">
        <id column="id" property="id" />
        <result column="openid" property="openid" />
        <result column="user_type" property="userType" />
        <result column="nickname" property="nickname" />
        <result column="gender" property="gender" />
        <result column="avatar" property="avatar" />
        <result column="mobile" property="mobile" />
        <result column="email" property="email" />
        <result column="country" property="country" />
        <result column="province" property="province" />
        <result column="city" property="city" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="unionid" property="unionid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, openid, user_type AS userType, nickname, gender, avatar, mobile, email, country, province, city, create_time AS createTime, update_time AS updateTime, unionid
    </sql>

</mapper>
